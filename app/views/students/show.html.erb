<p style="color: green"><%= notice %></p>

<%= render @student %>
<div>
    <%= link_to 'New Note', new_student_note_path(@student) %> | 
  <%= link_to 'Show Notes', student_notes_path(@student) %> | 
  <%= link_to 'Show Branches', student_branches_path(@student) %>

</div>

<div>
  <%= link_to "Edit this student", edit_student_path(@student) %> |
  <%= link_to "Back to students",  student_path(@student) %> |

  <%= button_to "Destroy this student", @student, method: :delete %>
</div>
