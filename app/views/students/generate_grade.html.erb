<%= form_with(url: generate_report_student_path(@student), method: :post) do |form| %>
  <fieldset>
    <legend>Select Semesters:</legend>
    <% @semesters.each do |semester| %>
      <div>
        <%= check_box_tag "semester_ids[]", semester.id %>
        <%= label_tag "semester_ids_#{semester.id}", semester.name_and_year %>
      </div>
    <% end %>
  </fieldset>
  <%= form.submit "Generate Report" %>
<% end %>
