<h1>Welcome to the Dashboard</h1>
<p>Please log in to continue.</p>

<%= form_with(scope: :session, url: sessions_create_path, local: true) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, required: true %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password, required: true %>
  </div>

  <div>
    <%= form.submit "Log in" %>
  </div>
<% end %>
